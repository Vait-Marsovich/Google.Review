            var Museum3_map;
            DG.then(function () {
                Museum3_map = DG.map('Museum3_map', {
                    center: [61.78483365488437,34.33084394806564],
                    zoom: 13
                });
                var myIcon = DG.icon({
                    iconUrl: 'https://pngimg.com/uploads/gps/gps_PNG42.png',
                    iconSize: [38, 50],
                });
                DG.marker([61.78483365488437,34.33084394806564], {icon: myIcon}).addTo(Museum3_map).bindPopup('Полёт');
            });
        </script>

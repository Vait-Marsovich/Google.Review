            var i = 0;
            function Places_open1(){
                document.getElementById("list_places").style = "transform: translateY(50%)";
                document.getElementById("read_more").style.color = 'white';
                document.getElementById("button").disabled = true;
                i = 0;
            }
            function Places_open2(){
                    if (i == 1){
                        document.getElementById("read_more").style.color = 'white';
                        document.getElementById("list_places").style = "transform: translateY(300%)";
                        document.getElementById("button").disabled = true;
                        i = 0;
                        document.getElementById("Beach").style.zIndex = "-1";
                    }
                    else {
                        document.getElementById("list_places").style = "transform: translateY(15%)";
                        document.getElementById("read_more").style.color = 'black';
                        document.getElementById("button").disabled = false;
                        i = 1;
                    }
            }
            function Beach_open(){
                var beach =  document.getElementById("Beach").style;
                beach.zIndex = "652143526143512435214365213612536123";
                var j = 0;
                (function count(j){
                    beach.opacity = j;
                    if (j < 1.01){
                        setTimeout(function() { count(j + 0.01); }, 5);
                    }
                })(0);
                document.getElementById("list_places").style.opacity = 0;
            }
            function Beach_close(){
                var beach1 = document.getElementById("Beach").style;
                var places = document.getElementById("list_places").style;
                places.opacity = 0;
                beach1.zIndex = "1000000";
                var k = 0;
                (function count_close(k){
                    places.opacity = k;
                    if (k < 1.01){
                        setTimeout(function() { count_close(k + 0.01); }, 5);
                    }
                })(0);
            }
